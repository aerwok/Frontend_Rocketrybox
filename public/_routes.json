{
  "version": 1,
  "routes": [
    {
      "src": "^/assets/(.+)\\.js$",
      "headers": { "Content-Type": "application/javascript" },
      "dest": "/assets/$1.js"
    },
    {
      "src": "^/(.+)\\.js$",
      "headers": { "Content-Type": "application/javascript" },
      "dest": "/$1.js"
    },
    {
      "handle": "filesystem"
    },
    {
      "src": ".*",
      "dest": "/index.html"
    }
  ]
} 